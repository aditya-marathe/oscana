{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "Oscana": {
            "format": "[ %(levelname)s ] ( %(name)s : %(asctime)s ) %(message)s",
            "datefmt": "%d/%m/%Y %H:%M:%S"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout",
            "formatter": "Oscana"
        },
        "file": {
            "class": "logging.handlers.RotatingFileHandler",
            "level": "DEBUG",
            "filename": "oscana.log",
            "formatter": "Oscana",
            "maxBytes": 10000,
            "backupCount": 5
        }
    }
}